# InhaleEase Configuration
# Copy this file to .env and fill in the actual values

# Server Port
PORT=5000

# Flask Application URL (for Node backend to proxy)
FLASK_URL=http://localhost:5005

# JWT Secret (change to a strong random value in production)
JWT_SECRET=your-super-secret-jwt-key-here

# Flask Session Secret (change to a strong random value in production)
FLASK_SECRET=your-super-secret-flask-key-here

# Node Environment
NODE_ENV=development
